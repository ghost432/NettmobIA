#DATABASE_NAME=flowise
#DATABASE_USER=flowise_user
#DATABASE_PASSWORD=votre_mot_de_passe_securise
#DATABASE_SSL=false
#DATABASE_SSL_KEY_BASE64=
#DATABASE_PATH=


############################################################################################################
############################################## SECRET KEYS #################################################
############################################################################################################
SECRETKEY_STORAGE_TYPE=file
SECRETKEY_PATH=./secretkey
FLOWISE_SECRETKEY_OVERWRITE=false

# Configuration AWS (si utilisation S3 pour les secrets)
SECRETKEY_AWS_ACCESS_KEY=
SECRETKEY_AWS_SECRET_KEY=
SECRETKEY_AWS_REGION=
SECRETKEY_AWS_NAME=

# ==================================================
# JOURNALISATION
# ==================================================
LOG_PATH=./logs
LOG_LEVEL=info

# ==================================================
# OUTILS ET DÉPENDANCES
# ==================================================
TOOL_FUNCTION_BUILTIN_DEP=true
TOOL_FUNCTION_EXTERNAL_DEP=true

# ==================================================
# STOCKAGE
# ==================================================
STORAGE_TYPE=local
BLOB_STORAGE_PATH=./storage

# Configuration S3 (optionnel)
S3_STORAGE_BUCKET_NAME=
S3_STORAGE_ACCESS_KEY_ID=
S3_STORAGE_SECRET_ACCESS_KEY=
S3_STORAGE_REGION=
S3_ENDPOINT_URL=
S3_FORCE_PATH_STYLE=false

# Configuration Google Cloud Storage (optionnel)
GOOGLE_CLOUD_STORAGE_CREDENTIAL=
GOOGLE_CLOUD_STORAGE_PROJ_ID=
GOOGLE_CLOUD_STORAGE_BUCKET_NAME=
GOOGLE_CLOUD_UNIFORM_BUCKET_ACCESS=false

# ==================================================
# SÉCURITÉ ET CORS
# ==================================================
NUMBER_OF_PROXIES=1
CORS_ORIGINS=*
IFRAME_ORIGINS=
FLOWISE_FILE_SIZE_LIMIT=50

# ==================================================
# FONCTIONNALITÉS
# ==================================================
SHOW_COMMUNITY_NODES=true
DISABLE_FLOWISE_TELEMETRY=false
DISABLED_NODES=
MODEL_LIST_CONFIG_JSON=

# ==================================================
# AUTHENTIFICATION JWT
# ==================================================
JWT_AUTH_TOKEN_SECRET=changez_ce_secret_jwt_tres_long_et_securise
JWT_REFRESH_TOKEN_SECRET=changez_ce_refresh_secret_tres_long_et_securise
JWT_ISSUER=flowise
JWT_AUDIENCE=flowise-users
JWT_TOKEN_EXPIRY_IN_MINUTES=1440
JWT_REFRESH_TOKEN_EXPIRY_IN_MINUTES=43200
EXPIRE_AUTH_TOKENS_ON_RESTART=false

# ==================================================
# SESSIONS ET MOTS DE PASSE
# ==================================================
EXPRESS_SESSION_SECRET=changez_ce_secret_session_tres_long_et_securise
PASSWORD_RESET_TOKEN_EXPIRY_IN_MINS=60
PASSWORD_SALT_HASH_ROUNDS=10
TOKEN_HASH_SECRET=changez_ce_hash_secret_tres_long_et_securise

# ==================================================
# EMAIL/SMTP
# ==================================================
SMTP_HOST=localhost
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_SECURE=false
ALLOW_UNAUTHORIZED_CERTS=false
SENDER_EMAIL=noreply@votre-domaine.fr

# ==================================================
# LICENCE ET ENVIRONNEMENT
# ==================================================
LICENSE_URL=
FLOWISE_EE_LICENSE_KEY=
OFFLINE=false
INVITE_TOKEN_EXPIRY_IN_HOURS=72
WORKSPACE_INVITE_TEMPLATE_PATH=

# ==================================================
# ANALYTIQUES
# ==================================================
POSTHOG_PUBLIC_API_KEY=
ENABLE_METRICS=false
METRICS_PROVIDER=open-telemetry
METRICS_INCLUDE_NODE_METRICS=false
METRICS_SERVICE_NAME=flowise
METRICS_OPEN_TELEMETRY_METRIC_ENDPOINT=
METRICS_OPEN_TELEMETRY_PROTOCOL=http
METRICS_OPEN_TELEMETRY_DEBUG=false

# ==================================================
# PROXY
# ==================================================
GLOBAL_AGENT_HTTP_PROXY=
GLOBAL_AGENT_HTTPS_PROXY=
GLOBAL_AGENT_NO_PROXY=

# ==================================================
# FILE D'ATTENTE (QUEUE)
# ==================================================
QUEUE_NAME=flowise-queue
QUEUE_REDIS_EVENT_STREAM_MAX_LEN=10000
WORKER_CONCURRENCY=1
REMOVE_ON_AGE=3600
REMOVE_ON_COUNT=1000

# ==================================================
# REDIS (optionnel - pour file d'attente)
# ==================================================
REDIS_URL=
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=
REDIS_TLS=false
REDIS_CERT=
REDIS_KEY=
REDIS_CA=
REDIS_KEEP_ALIVE=30000
ENABLE_BULLMQ_DASHBOARD=false