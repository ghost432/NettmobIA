PORT=3000
MODE=production
APP_URL=https://france.nettmobinfotech.fr/flowise
DEBUG=false

# APIKEY_PATH=/your_apikey_path/.flowise # (will be deprecated by end of 2025)

############################################################################################################
############################################## DATABASE ####################################################
############################################################################################################

 DATABASE_TYPE=sqlite
 DATABASE_PATH=./database
# DATABASE_PORT=
# DATABASE_HOST=
# DATABASE_NAME=flowise
# DATABASE_USER=
# DATABASE_PASSWORD=
# DATABASE_SSL=false
# DATABASE_SSL_KEY_BASE64=

# Pour MySQL/PostgreSQL (décommentez si nécessaire)
#DATABASE_TYPE=mysql
#DATABASE_HOST=localhost
